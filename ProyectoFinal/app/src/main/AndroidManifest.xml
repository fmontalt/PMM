<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="come.example.fraleo.proyectofinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterAccountActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginSuccessActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Actividad_fragmento" />
        <activity
            android:name=".PantallaOpciones"
            android:label="@string/title_activity_pantalla_opciones"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Recibo_juego" />
        <activity android:name=".factura_personal" />
        <activity android:name=".acercade"></activity>
    </application>

</manifest>